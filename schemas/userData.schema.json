{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "Name": "string",
            "email": "user@example.com",
            "emailVerified":true,
            "createDate": "2019-08-24",
            "accountName": "string",
            "password": "pa$$word"
        }
    ],
    "required": [
        "Name",
        "email",
        "emailVerified",
        "createDate",
        "accountName",
        "password"
    ],
    "properties": {
        "Name": {
            "$id": "#/properties/Name",
            "type": "string",
            "minLength": 3,
            "title": "The Name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "string"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "minLength": 5,
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "user@example.com"
            ]
        },
        "emailVerified": {
            "$id": "#/properties/emailVerified",
            "type": "boolean",
            "title": "The emailVerified schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                true
            ]
        },
        "createDate": {
            "$id": "#/properties/createDate",
            "type": "string",
            "title": "The createDate schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "2019-08-24"
            ]
        },
        "accountName": {
            "$id": "#/properties/accountName",
            "type": "string",
            "minLength": 3,
            "title": "The accountName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "string"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "minLength": 3,
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "pa$$word"
            ]
        }
    },
    "additionalProperties": true
}
